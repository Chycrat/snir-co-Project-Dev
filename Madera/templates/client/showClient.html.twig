{% extends "base.html.twig" %}
{% block stylesheets %} 
    <link rel="canonical" href="https://getbootstrap.com/docs/4.0/components/scrollspy/">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">  
{% endblock %}
{% block body %} 

    <div class="container mt-5" margin-top="15dp">
        <h1><span class="label label-default">Client</span></h1>
        <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ client.id }}</td>
            </tr>
            <tr>
                <th>Id_client</th>
                <td>{{ client.idClient }}</td>
            </tr>
            <tr>
                <th>Nom_client</th>
                <td>{{ client.nomClient }}</td>
            </tr>
            <tr>
                <th>Code_client</th>
                <td>{{ client.codeClient }}</td>
            </tr>
            <tr>
                <th>Mdp_client</th>
                <td>{{ client.mdpClient }}</td>
            </tr>
            <tr>
                <th>Telephone_client</th>
                <td>{{ client.telephoneClient }}</td>
            </tr>
            <tr>
                <th>Adresse_client</th>
                <td>{{ client.adresseClient }}</td>
            </tr>
            <tr>
                <th>Email_client</th>
                <td>{{ client.emailClient }}</td>
            </tr>
        </tbody>
    </table>
    </div>

{% endblock %}
{% block javascripts %}{% endblock %}