{% extends 'base.html.twig' %}

{% block title %}MaderaClient{% endblock %}

{% block body %}
    

    <div class="container mt-5" margin-top="15dp">
        <h1><span class="label label-default">Client</span></h1>
        <table class="table">
            <tbody>
                <tr>
                    <th>Nom_client</th>
                    <td>{{ madera_client.nomClient }}</td>
                </tr>
                <tr>
                    <th>Code_client</th>
                    <td>{{ madera_client.codeClient }}</td>
                </tr>
                <tr>
                    <th>Telephone_client</th>
                    <td>{{ madera_client.telephoneClient }}</td>
                </tr>
                <tr>
                    <th>Adresse_client</th>
                    <td>{{ madera_client.adresseClient }}</td>
                </tr>
                <tr>
                    <th>Email_client</th>
                    <td>{{ madera_client.emailClient }}</td>
                </tr>
            </tbody>
        </table>
        <a class="btn btn-success" href="{{ path('madera_client_index') }}">back to list</a>
        <a class="btn btn-success" href="{{ path('madera_client_edit', {'id': madera_client.id}) }}">edit</a>

        {{ include('madera_client/_delete_form.html.twig') }}
    </div>

   
{% endblock %}
